# Variables de entorno para automation-hub

# Nivel de logging (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Entorno de ejecución
ENVIRONMENT=development

# Zona horaria
TZ=UTC

# ============================================
# Supabase Configuration
# ============================================
SUPABASE_URL=
SUPABASE_KEY=

# ============================================
# Google OAuth Configuration
# ============================================
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GBP_REFRESH_TOKEN=

# ============================================
# GBP Jobs Configuration
# ============================================
# Filtro opcional por tenant (si viene, filtra por este nombre_nora)
GBP_NOMBRE_NORA=

# Métricas a obtener (separadas por coma)
GBP_METRICS=WEBSITE_CLICKS,CALL_CLICKS

# Días hacia atrás para obtener métricas (default: 30)
GBP_DAYS_BACK=30
